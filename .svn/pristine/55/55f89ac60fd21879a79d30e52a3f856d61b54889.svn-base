.label-main{
    font-size: 20px;
    color:black;
    font-weight: bold;
        }
.frame-padding{
    margin-left: 0px; 
    margin-right: 0px;  
    margin-bottom: 0px;
    margin-top: 40px;   
    }
.main-border{
    border-color: rgb(35, 134, 35);
    }
.form-control {
        display: block;
        width: 100%;
        margin-bottom: 10px;
        font-size: 0.875rem;
        line-height: 1.25;
        color: #3e515b;
        background-color: #fff;
        background-image: none;
        background-clip: padding-box;
        border: 1px solid #c2cfd6;
        border-radius: 0;
        transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
    }
