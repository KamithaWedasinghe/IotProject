<div class="container">
  <div class="row">
    <div class="btn col-md-2">
      <button *ngIf="clicked1" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">1</span>
      </button>
      <button *ngIf="default1" type="button" class="btn btn-primary btn-circle">
        <span class="payroll-run-number">1</span>
      </button>

      <div *ngIf="clicked1" class="heading-clicked" style="margin-left: 4%;">Create Payroll-run</div>
      <div *ngIf="default1" class="heading-default">Create Payroll-run</div>
    </div>
    <div class="btn col-md-2">
      <button *ngIf="clicked2" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">2</span>
      </button>
      <button *ngIf="default2" type="button" class="btn btn-primary btn-circle">
        <span class="payroll-run-number">2</span>
      </button>

      <div *ngIf="clicked2" style="margin-left: 4%;" class="heading-clicked">Check-List</div>
      <div *ngIf="default2" class="heading-default">Check-List</div>
    </div>
    <div class="btn col-md-2">
      <button *ngIf="clicked3" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">3</span>
      </button>
      <button *ngIf="default3" type="button" class="btn btn-primary btn-circle">
        <span class="payroll-run-number">3</span>
      </button>
      <button *ngIf="dryRunMoreDetailsForView" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">3</span>
      </button>
      <button *ngIf="dryRunerrorDetailsForView" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">3</span>
      </button>
      <button *ngIf="viewOneEmployeeDeatils" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">3</span>
      </button>
      <button *ngIf="viewOneEmployeeErrorDeatils" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">3</span>
      </button>



      <div *ngIf="clicked3" style="margin-left: 4%;" class="heading-clicked">Payroll Dry-Run</div>
      <div *ngIf="default3" class="heading-default">Payroll Dry-Run</div>
      <div *ngIf="dryRunMoreDetailsForView" class="heading-clicked">Payroll Dry-Run</div>
      <div *ngIf="dryRunerrorDetailsForView" class="heading-clicked">Payroll Dry-Run</div>
      <div *ngIf="viewOneEmployeeDeatils" class="heading-clicked">Payroll Dry-Run</div>
      <div *ngIf="viewOneEmployeeErrorDeatils" class="heading-clicked">Payroll Dry-Run</div>

    </div>
    <div class="btn col-md-2">
      <button *ngIf="clicked4" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">4</span>
      </button>
      <button *ngIf="default4" type="button" class="btn btn-primary btn-circle">
        <span class="payroll-run-number">4</span>
      </button>
      <button *ngIf="completedPayrollRunDetailsForView" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">4</span>
      </button>
      <button *ngIf="errorPayrollRunMoreDetailForView" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">4</span>
      </button>
      <button *ngIf="viewOneEmployeeOfFinalDeatils" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">4</span>
      </button>
      <button *ngIf="viewOneEmployeeErrorOfFinalDeatils" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">4</span>
      </button>

      <div *ngIf="clicked4" style="margin-left: 4%;" class="heading-clicked">Payroll-Run</div>
      <div *ngIf="default4" class="heading-default">Payroll-Run</div>
      <div *ngIf="completedPayrollRunDetailsForView" class="heading-clicked">Payroll-Run</div>
      <div *ngIf="errorPayrollRunMoreDetailForView" class="heading-clicked">Payroll-Run</div>
      <div *ngIf="viewOneEmployeeOfFinalDeatils" class="heading-clicked">Payroll-Run</div>
      <div *ngIf="viewOneEmployeeErrorOfFinalDeatils" class="heading-clicked">Payroll-Run</div>
    </div>
    <div class="btn col-md-2">
      <button *ngIf="clicked5" type="button" class="btn btn-success btn-circle">
        <span class="payroll-run-number">5</span>
      </button>
      <button *ngIf="default5" type="button" class="btn btn-primary btn-circle">
        <span class="payroll-run-number">5</span>
      </button>
      <!-- <svg height="120" width="120">
        <circle cx="80" cy="60" r="40" stroke="black" stroke-width="0" fill="#336699" />
      </svg> -->
      <div *ngIf="clicked5" style="margin-left: 4%;" class="heading-clicked">Post-Run</div>
      <div *ngIf="default5" class="heading-default">Post-Run</div>
    </div>
  </div>
  <div class="row">
    <!-- <div class="col-md-2"></div> -->
    <div *ngIf="clicked1" class="col-md-8 detail-body">
      <form #form="ngForm" (ngSubmit)="createPayrollRun(form)">
        <div class="form-group row">
          <div class="col-md-5">
            <p class="title-label"> Select Month</p>
          </div>
          <div class="col-md-5">
            <select id="select" class="form-control input-lg" size="1" name="payrollrunmonth" ngModel required>
              <option *ngFor="let displayMonthArray of displayMonthArray">{{displayMonthArray.name}}</option>
            </select>
          </div>
        </div>
        <div class="form-group row">
          <div class="col-md-5">
            <p class="title-label"> Start Date</p>
          </div>
          <div class="col-md-5">
            <my-date-picker name="currentDateTime" [options]="myDatePickerOptions" [(ngModel)]="currentDateTime" ngModel required></my-date-picker>
          </div>
        </div>
        <div class="form-group row">
          <div class="col-md-5">
          </div>
          <div class="col-md-5">
            <button type="submit" class="btn btn-success">Create payroll-run</button>
          </div>

        </div>
      </form>
    </div>
    <div *ngIf="clicked2" class="col-md-8 checklist">
      <div class="row">
        <div class="col-md-12">
          <div *ngFor="let checkList of checkList" class="checkbox">
            <label class="payroll-run-checkList">
              <input type="checkbox" [disabled]="checkList.isSelect" (change)="updatePayRollChecklist(checkList)" [(ngModel)]="checkList.isSelect"
                value="checkList.name"> {{checkList.name}}</label>
          </div>
        </div>
      </div>
      <div class="form-group row">
        <div class="col-md-6">
        </div>
        <div class="btn-sublit-checklist col-md-6">
          <button type="submit" (click)="checkFinised()" class="btn btn-success">Check Finished</button>
        </div>
      </div>
    </div>
    <div *ngIf="clicked3" class="col-md-12 dryrun">
      <div class="row frame-padding">
        <div class="col-md-3"></div>
        <div class="col-md-6">
          <div class="card main-border">
            <div class="card-block">
              <button type="submit" (click)="startDryRun()" class="btn btn-success Startbuttion-align" [disabled]="errorDryRunCount != 0 || createdDryRunCount != 0">Start Dry-Run</button>
              <button type="submit" (click)="finishDryRun()" class="btn btn-success Finishbuttion-align" [disabled]="">Finish Dry-Run</button>
              <div class="padding-colu">
                <div class="row">
                  <div class="col-md-9">
                    <label class="label-internal" name="createdDryRunCount">Created Dry Run Count :&nbsp;&nbsp;&nbsp;
                      <b class="label-ready">{{createdDryRunCount}}</b>
                    </label>
                  </div>
                  <div class="col-md-3">
                    <button *ngIf="createdDryRunCount != 0" type="submit" (click)="completedMoreDetails()" class="btn btn-success">More Details</button>
                    <button *ngIf="createdDryRunCount == 0" type="submit" (click)="completedMoreDetails()" class="btn btn-primary" [disabled]="errorDryRunCount == 0">More Details</button>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-9">
                    <label class="label-internal" name="errorDryRunCount">Error Dry Run Count &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;:&nbsp;&nbsp;&nbsp;
                      <b class="label-error">{{errorDryRunCount}}</b>

                    </label>
                  </div>
                  <div class="col-md-3">
                    <button *ngIf="errorDryRunCount != 0" type="submit" (click)="errorMoreDetails()" class="btn btn-success">More Details</button>
                    <button *ngIf="errorDryRunCount == 0" type="submit" (click)="errorMoreDetails()" class="btn btn-primary" [disabled]="errorDryRunCount == 0">More Details</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-3"></div>
    </div>

    <div *ngIf="clicked4" class="col-md-12 dryrun">
      <div class="row frame-padding">
        <div class="col-md-3"></div>
        <div class="col-md-6">
          <div class="card main-border">
            <div class="card-block">
              <button type="submit" (click)="startRun()" class="btn btn-success Startbuttion-align" [disabled]="errorPayrollRunCount != 0 || createdPayrollRunCount != 0">Start Run</button>
              <button type="submit" (click)="finishRun()" class="btn btn-success Finishbuttion-align">Finish Run</button>
              <div class="padding-colu">
                <div class="row">
                  <div class="col-md-9">
                    <label class="label-internal" name="createdPayrollRunCount">Created Payroll Run Count :&nbsp;&nbsp;&nbsp;
                      <b class="label-ready">{{createdPayrollRunCount}}</b>
                    </label>
                  </div>
                  <div class="col-md-3">
                    <button *ngIf="createdPayrollRunCount != 0" type="submit" (click)="completedPayrollRunMoreDetails()" class="btn btn-success">More Details</button>
                    <button *ngIf="createdPayrollRunCount == 0" type="submit" (click)="completedPayrollRunMoreDetails()" class="btn btn-primary"
                      [disabled]="errorPayrollRunCount == 0">More Details</button>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-9">
                    <label class="label-internal" name="errorPayrollRunCount">Error Payroll Run Count &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;:&nbsp;&nbsp;&nbsp;
                      <b class="label-error">{{errorPayrollRunCount}}</b>

                    </label>
                  </div>
                  <div class="col-md-3">
                    <button *ngIf="errorPayrollRunCount != 0" type="submit" (click)="errorPayrollRunMoreDetails()" class="btn btn-success">More Details</button>
                    <button *ngIf="errorPayrollRunCount == 0" type="submit" (click)="errorPayrollRunMoreDetails()" class="btn btn-primary" [disabled]="errorPayrollRunCount == 0">More Details</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-3"></div>
    </div>
  </div>
  <!-- post run start -->
  <app-payroll-post-run></app-payroll-post-run>
  <div class="row">
    <div class="col-sm-6">
    </div>
    <div class="col-sm-6">
        <button type="button" (click)="finishPayrollRun()" class="btn btn-secondary finish-btn">Finish</button>
    </div>
  </div>
  <!-- post run end -->




  <div *ngIf="dryRunMoreDetailsForView">

    <div class="card main-border">
      <div class="card-block">
        <div class="row">
          <button type="button" (click)="BacktoDryRun()" class="btn btn-primary btn-sm moreDetails-backButtion">Back</button>
          <label class="label-MoreDetails">Created Payroll More Details</label>
        </div>
        <hr class="hr1">

        <table class="table table-striped" [mfData]="completeDryRundetailsArry | dataFilter : filterQuery" #mf="mfDataTable" [mfRowsOnPage]="5">
          <thead>
            <tr>
              <p>
                <label class="">Filter by name:</label>
                <input class="form-control" [(ngModel)]="filterQuery" />
              </p>
            </tr>

            <tr>
              <th style="width: 80%">
                <mfDefaultSorter by="name">Name</mfDefaultSorter>
              </th>
              <th style="width: 20%">
                <mfDefaultSorter by=""></mfDefaultSorter>
              </th>

            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let item of mf.data;let i=index">
              <td>{{item.fullName}}</td>
              <td>
                <button type="button" data-container="body" data-toggle="popover" data-placement="left" data-content="Vivamus sagittis lacus vel augue laoreet rutrum faucibus."
                  class="btn btn-success btn-sm button-sizes" (click)="viewReadyDetails(i)">Details</button>
              </td>

            </tr>
          </tbody>
          <tfoot>
            <tr>
              <td colspan="4">
                <mfBootstrapPaginator [rowsOnPageSet]="[5,10,25]"></mfBootstrapPaginator>
              </td>
            </tr>
          </tfoot>
        </table>
      </div>
    </div>
  </div>
  <div *ngIf="dryRunerrorDetailsForView">

    <div class="card main-border">
      <div class="card-block">
        <div class="row">
          <button type="button" (click)="BacktoDryRun()" class="btn btn-primary btn-sm moreDetails-backButtion">Back</button>
          <label class="label-MoreDetails">Error Payroll Details</label>
        </div>



        <table class="table table-striped" [mfData]="errorDryRundetailsArry | dataFilter : filterQuery" #mf="mfDataTable" [mfRowsOnPage]="5">
          <thead>
            <tr>
              <p>
                <label class="">Filter by name:</label>
                <input class="form-control" [(ngModel)]="filterQuery" />
              </p>
            </tr>
            <tr>
              <th style="width: 80%">
                <mfDefaultSorter by="name">Name</mfDefaultSorter>
              </th>
              <th style="width: 20%">
                <mfDefaultSorter by=""></mfDefaultSorter>
              </th>
            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let item of mf.data;let i=index">
              <td>{{item.fullName}}</td>
              <td>
                <button type="button" data-container="body" data-toggle="popover" data-placement="left" data-content="Vivamus sagittis lacus vel augue laoreet rutrum faucibus."
                  class="btn btn-success btn-sm button-sizes" (click)="viewErrorDetails(i)">Details</button>
              </td>
            </tr>
          </tbody>
          <tfoot>
            <tr>
              <td colspan="4">
                <mfBootstrapPaginator [rowsOnPageSet]="[5,10,25]"></mfBootstrapPaginator>
              </td>
            </tr>
          </tfoot>
        </table>
      </div>
    </div>
  </div>

  <div *ngIf="viewOneEmployeeDeatils">

