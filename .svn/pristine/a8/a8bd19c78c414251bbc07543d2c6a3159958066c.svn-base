  <div *ngIf="completedPayrollRunDetailsForView">

    <div class="card main-border">
      <div class="card-block">
        <div class="row">
          <button type="button" (click)="BackPayrollRun()" class="btn btn-primary btn-sm moreDetails-backButtion">Back</button>
          <label class="label-MoreDetails">Created Payroll More Details</label>
        </div>
        <hr class="hr1">

        <table class="table table-striped" [mfData]="completePayrollRundetailsArry | dataFilter : filterQuery" #mf="mfDataTable"
          [mfRowsOnPage]="5">
          <thead>
            <tr>
              <p>
                <label class="">Filter by name:</label>
                <input class="form-control" [(ngModel)]="filterQuery" />
              </p>
            </tr>

            <tr>
              <th style="width: 80%">
                <mfDefaultSorter by="name">Name</mfDefaultSorter>
              </th>
              <th style="width: 20%">
                <mfDefaultSorter by=""></mfDefaultSorter>
              </th>

            </tr>
          </thead>
          <tbody>
            <tr *ngFor="let item of mf.data;let i=index">
              <td>{{item.fullName}}</td>
